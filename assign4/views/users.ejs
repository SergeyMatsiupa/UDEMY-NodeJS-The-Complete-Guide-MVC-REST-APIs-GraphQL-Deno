<%- include("includes/head.ejs") %>
<link href="/css/users.css" rel="stylesheet">
</head>
<body>
    <%- include("includes/header.ejs")  %>
    <main>
        <% if (users.length>0) { %>
            <h1>Users:</h1>
            <ul class="users_list">
                <% for (let user of users) {%>
                    <li><%=user.username%></li>
                <% } %>
            </ul>
        <% } else { %>
        <h1>No users yet...</h1>
        <% } %>
    </main>
<%- include("includes/end.ejs")  %>